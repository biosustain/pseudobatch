@article{sanderAllostericFeedbackInhibition2019,
	title = {Allosteric {Feedback} {Inhibition} {Enables} {Robust} {Amino} {Acid} {Biosynthesis} in {E}. coli by {Enforcing} {Enzyme} {Overabundance}},
