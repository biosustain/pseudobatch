@article{krantzDataManagementModeling2021,
	title = {Data {Management} and {Modeling} in {Plant} {Biology}},
	volume = {12},
	issn = {1664-462X},
	url = {https://www.frontiersin.org/articles/10.3389/fpls.2021.717958/full},
	doi = {10.3389/fpls.2021.717958},
	abstract = {The study of plant-environment interactions is a multidisciplinary research field. With the emergence of quantitative large-scale and high-throughput techniques, amount and dimensionality of experimental data have strongly increased. Appropriate strategies for data storage, management, and evaluation are needed to make efficient use of experimental findings. Computational approaches of data mining are essential for deriving statistical trends and signatures contained in data matrices. Although, current biology is challenged by high data dimensionality in general, this is particularly true for plant biology. Plants as sessile organisms have to cope with environmental fluctuations. This typically results in strong dynamics of metabolite and protein concentrations which are often challenging to quantify. Summarizing experimental output results in complex data arrays, which need computational statistics and numerical methods for building quantitative models. Experimental findings need to be combined by computational models to gain a mechanistic understanding of plant metabolism. For this, bioinformatics and mathematics need to be combined with experimental setups in physiology, biochemistry, and molecular biology. This review presents and discusses concepts at the interface of experiment and computation, which are likely to shape current and future plant biology. Finally, this interface is discussed with regard to its capabilities and limitations to develop a quantitative model of plant-environment interactions.},
	language = {en},
	urldate = {2022-06-30},
	journal = {Frontiers in Plant Science},
	author = {Krantz, Maria and Zimmer, David and Adler, Stephan O. and Kitashova, Anastasia and Klipp, Edda and Mühlhaus, Timo and Nägele, Thomas},
	month = sep,
	year = {2021},
	pages = {717958},
}

@inproceedings{petraschEnterpriseDataManagement2018a,
	title = {Towards {Enterprise} {Data} {Management}: {Data} {Model} {Mapping}},
	shorttitle = {Towards {Enterprise} {Data} {Management}},
	doi = {10.1109/WIECON-ECE.2018.8783035},
	abstract = {Within an organization, different information systems, e.g. Enterprise Resource Planning, use their own, specific, and proprietary data model. Enterprise data management enables a 360-degree view of customers, products and other data elements but a holistic approach is needed: All the different data models need to be “mapped” to one consolidated, normalized enterprise data model in order to enable a real-time transformation, i.e. data elements of the information systems need to be converted, mapped, merged, and stored in an enterprise database. In the era of Big Data, IoT, and Industry 4.0, EDM becomes an even more important task in the context of Digitalization. This paper describes a meta-model-based approach that lets the data scientist map elements of different data models to elements of an enterprise data model using UML class diagrams, the UML Profile mechanism, OCL, and model transformations. This leads to an executable data mapper for Enterprise data management that transfers and consolidated data from operational information systems into an enterprise database.},
	booktitle = {2018 {IEEE} {International} {WIE} {Conference} on {Electrical} and {Computer} {Engineering} ({WIECON}-{ECE})},
	author = {Petrasch, Roland and Kongkachandra, Rachada},
	month = dec,
	year = {2018},
	keywords = {Big Data, Data Mapping, Data Model, Data Model Mapping, Enterprise data management, Enterprise data model, Metamodel, Model Transformation, UML Class Diagram},
	pages = {72--74},
}

